@using Pokewordle.Shared.HtmlUtil;

<tr>
    @if (TableCells is null)
    {
        <td>No Data</td>
    } else
    {
        @foreach (ITableCell tableCell in TableCells)
        {
            <TemplateCell TableCell=tableCell></TemplateCell>
        }
    }
</tr>

@code {
    [Parameter]
    [EditorRequired]
    public IList<ITableCell>? TableCells { get; set; }
}
